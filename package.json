{
  "name": "shopeeclonewatch",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "cross-env BUILD_MODE=dev vite",
    "dev:win": "cross-env BUILD_MODE=dev NODE_OPTIONS=\"--max-old-space-size=4096\" VITE_FORCE_COLOR=1 vite --force",
    "dev:safe": "cross-env BUILD_MODE=dev vite --force --clearScreen false",
    "dev:bypass": "cross-env BUILD_MODE=dev NODE_OPTIONS=\"--max-old-space-size=4096\" vite --force --config ./vite.config.ts",
    "build": "cross-env BUILD_MODE=production tsc && vite build",
    "build:production": "cross-env BUILD_MODE=production NODE_ENV=production tsc && vite build",
    "deploy": "pnpm run build:production && pnpm run preview",
    "preview": "vite preview",
    "lint": "eslint --ext ts,tsx src/",
    "lint:fix": "eslint --fix --ext ts,tsx src/",
    "prettier": "prettier --check \"src/**/(*.tsx|*.ts|*.css|*.scss)\"",
    "prettier:fix": "prettier --write \"src/**/(*.tsx|*.ts|*.css|*.scss)\"",
    "test": "vitest",
    "test:unit": "vitest run src/",
    "test:integration": "vitest run test/integration/",
    "test:e2e": "vitest run test/e2e/",
    "test:snapshots": "vitest run test/snapshots/",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@floating-ui/react": "^0.27.18",
    "@heroicons/react": "^2.2.0",
    "@heroui/system": "^2.4.25",
    "@heroui/tooltip": "^2.2.26",
    "@hookform/resolvers": "^5.2.2",
    "@tanstack/react-query": "^5.90.21",
    "@tanstack/react-query-devtools": "^5.91.3",
    "axios": "^1.13.6",
    "classnames": "^2.5.1",
    "cross-env": "^7.0.3",
    "date-fns": "^4.1.0",
    "dompurify": "^3.3.1",
    "framer-motion": "^11.15.0",
    "html-to-text": "^9.0.5",
    "i18next": "^25.8.13",
    "immer": "^11.1.4",
    "lodash": "^4.17.23",
    "nuqs": "^2.8.8",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.54.2",
