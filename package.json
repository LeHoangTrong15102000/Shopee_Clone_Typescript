{
  "name": "shopeeclonewatch",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "cross-env BUILD_MODE=dev vite",
    "dev:win": "cross-env BUILD_MODE=dev NODE_OPTIONS=\"--max-old-space-size=4096\" VITE_FORCE_COLOR=1 vite --force",
    "dev:safe": "cross-env BUILD_MODE=dev vite --force --clearScreen false",
    "dev:bypass": "cross-env BUILD_MODE=dev NODE_OPTIONS=\"--max-old-space-size=4096\" vite --force --config ./vite.config.ts",
    "build": "cross-env BUILD_MODE=production tsc && vite build",
    "build:production": "cross-env BUILD_MODE=production NODE_ENV=production tsc && vite build",
    "deploy": "pnpm run build:production && pnpm run preview",
    "preview": "vite preview",
    "lint": "eslint --ext ts,tsx src/",
    "lint:fix": "eslint --fix --ext ts,tsx src/",
    "prettier": "prettier --check \"src/**/(*.tsx|*.ts|*.css|*.scss)\"",
    "prettier:fix": "prettier --write \"src/**/(*.tsx|*.ts|*.css|*.scss)\"",
    "test": "vitest",
    "test:unit": "vitest src/",
    "test:integration": "vitest test/integration/",
    "test:e2e": "vitest test/e2e/",
    "test:snapshots": "vitest test/snapshots/",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@floating-ui/react": "^0.26.24",
    "@heroui/react": "^2.7.10",
    "@hookform/resolvers": "^3.9.1",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tanstack/react-query": "^5.62.3",
    "@tanstack/react-query-devtools": "^5.62.3",
    "@tippyjs/react": "^4.2.6",
    "axios": "^1.7.9",
    "classnames": "^2.5.1",
    "cross-env": "^7.0.3",
    "date-fns": "^2.30.0",
    "dompurify": "^3.2.2",
    "framer-motion": "^11.15.0",
    "html-to-text": "^9.0.5",
    "i18next": "^23.16.8",
    "immer": "^10.1.1",
    "lodash": "^4.17.21",
    "query-string": "^9.1.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.54.2",
